// CloudUserAccessAdmin
// Enables provisioning of IAM roles.
{
    "Version": "2012-10-17",
    "Statement": [
        {
			"Sid": "CloudUserAccessAdmin20191211",
            "Effect": "Allow",
            "Action": [
				"iam:*",
				"organizations:*",
				"sts:DecodeAuthorizationMessage",
				"support:*"
			],
            "Resource": "*"
        }
    ]
}